@model IEnumerable<WebApp.Models.Questionnaire>

@{
    ViewData["Title"] = "Home";
}

<h1>Questionnaires</h1>

<ul>
    @foreach (var questionnaire in Model)
    {
        <li>
            <a asp-controller="Questionnaire" asp-action="Fill" asp-route-id="@questionnaire.Id">@questionnaire.Title</a>
        </li>
    }
</ul>